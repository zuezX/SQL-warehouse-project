create table bronze.crm_cust_info(
cst_id INT,
cst_key NVARCHAR (50),
cst_firstname NVARCHAR (50),
cst_ lastname NVARCHAR (50) ,
cst_material_status NVARCHAR (50),
cst_gndr NVARCHAR (50) ,
cst_create_date date 
);

CREATE TABLE bronze. crm_pra_info (
prd_id INT,
prd_key NVARCHAR (50) ,
prd_nm NVARCHAR (50),
prd_cost INT,
prd_line NVARCHAR (50),
prd_start_at DATETIME, prd_end_dt DATETIME
);
 CREATE TABLE bronze.crm_sales_details ( s1s_ord_num NVARCHAR (50),
sls_prd_key NVARCHAR(50),
sls_cust_id INT,
s1s_order_dt INT,
s1s_ship_dt INT,
sls_due_dt INT,
s1s_sales INT,
sls_quantity INT,
sls_price INT
);

CREATE TABLE bronze. erp_loc_a101  (
cid NVARCHAR (50),
cntry NVARCHAR (50)
);

CREATE TABLE bronze. erp_cust_az12 ( 
cid NVARCHAR (50),
bdate DATE, 
gen NVARCHAR ( 50)
);
CREATE TABLE bronze. erp_px_cat_glv2 (
id NVARCHAR (50),
cat NVARCHAR (50),
subcat NVARCHAR (50),
maintenance NVARCHAR ( 50)
);
